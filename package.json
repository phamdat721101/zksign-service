{
  "name": "zksign-service",
  "version": "1.0.0",
  "description": "A backend service for uploading files to IPFS with encryption and decryption support using a view key.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon index.ts",
    "run": "ts-node index.ts",
    "vercel-build": "tsc"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "express": "^4.21.2",
    "ipfs-http-client": "^54.0.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.4.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "vercel": {
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "index.ts" }
    ]
  },
  "license": "MIT"
}
